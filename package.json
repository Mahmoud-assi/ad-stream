{
  "name": "adstream",
  "version": "1.0.38",
  "description": "Add a stream of ads with Add-Stream.",
  "keywords": [
    "react",
    "ads",
    "iframe",
    "mui",
    "component",
    "emotion",
    "ad-stream",
    "add-stream"
  ],
  "license": "MIT",
  "author": "Add-stream <add6stream@gmail.com>",
  "type": "commonjs",
  "sideEffects": [
    "./src/web-component.tsx",
    "*.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./browser": {
      "default": "./dist/browser/web-component.global.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/browser"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^5.18.0",
    "crypto-js": "^4.2.0",
    "keen-slider": "^6.8.6",
    "react-to-webcomponent": "^2.0.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  }
}
